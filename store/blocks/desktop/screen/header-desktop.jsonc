{
    "header-layout.desktop": {
        "title": "Header Desktop",
        "children": ["header-row#header_top","header-row#header_middle"]
      },
      "header-row#header_top": {
        "title": "Header superior",
        "children": [
          "logo#header_top-d--logo",
          "header-spacer",
          "search-bar",
          "login",
          //"modal-trigger#header_top-d--login",
          "minicart.v2"
        ],
        "props": {
          "blockClass": "header_top-d"
        }
      },
      "logo#header_top-d--logo": {
        "title": "logo",
        "props": {
          "url": "assets/img/logo-easy.jpg",
          "width": 100,
          "href": "/"
        }
      },
      "modal-trigger#header_top-d--login": {
        "children": [
          "rich-text#header_top-d--login",
          "modal-layout#header_top-d--login"
        ]
      },
    
      "rich-text#header_top-d--login": {
        "props": {
          "text": "login"
        }
      },
      "modal-layout#header_top-d--login": {
        "children": ["login-content"]
      },
      "login": {
        "props": {
          "emailAndPasswordTitle": "LOG-IN",
          "accessCodeTitle": "Acess Code LOG-IN",
          "emailPlaceholder": "e-mail",
          "passwordPlaceholder": "password",
          "showPasswordVerificationIntoTooltip": true,
          "showIconProfile": true
        }
      },
    
      "header-row#header_middle": {
        "title": "Header medio",
        "children": [
          "drawer#header_desktop--menu",
          "rich-text#catalogo"
        ],
        "props": {
          "blockClass": "header_middle"
        }
      },
      "autocomplete-result-list.v2":{
        "blocks":["product-summary"]
      },
      "rich-text#catalogo":{
        "props":{
          "text":"Cat√°logo"
        }
      }
    
}